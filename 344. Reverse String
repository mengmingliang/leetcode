char* reverseString(char* s) {
    char *end = NULL;
    char *start = s;
    char *rtval = s;
    char c;
    
    while((*s)!='\0'){
        end = s;
        s++;
    }
    
    while(start<end){
        c = *start;
        (*start) = (*end);
        (*end) = c;
        start++;
        end--;
    }
    
    return rtval;
}
